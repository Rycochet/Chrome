{
	"name": "__MSG_extName__",
	"short_name": "t2tf",
	"version": "2.6.2",
	"manifest_version": 2,
	"minimum_chrome_version": "40",
	"homepage_url": "https://chrome.google.com/webstore/detail/tabs-to-the-front/hiembaoomcehoiehhdldabfgnmphappc",
	"author": "Ryc O'Chet <rycochet@rycochet.com>",
	"permissions": [
		"tabs",
		"storage"
	],
	"optional_permissions": [
		"<all_urls>"
	],
	"default_locale": "en",
	"description": "__MSG_extDescription__",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"options_page": "options.html",
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"browser_action": {
		"default_title": "__MSG_extTitleEnabled__",
		"default_icon": "icon48.png"
	},
	"background": {
		"persistent": true,
		"scripts": ["background.js"]
	},
	"commands": {
		"toggle": {
			"suggested_key": {
				"default": "Alt+T"
			},
			"description": "__MSG_keyboardShortcutToggle__"
		},
		"ignore": {
			"suggested_key": {
				"default": "Alt+Shift+T"
			},
			"description": "__MSG_keyboardShortcutIgnore__"
		}
	}
}
